{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"outputs": [
				"dist/**"
			]
		},
		"dev": {
			"inputs": [
				"src/**",
				"public/**"
			]
		},
		"dev:editor": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"src/**",
				"public/**"
			],
			"cache": false,
			"persistent": true
		},
		"lint": {}
	}
}